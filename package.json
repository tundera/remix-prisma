{
  "private": true,
  "name": "remix-worker-template",
  "main": "./worker.js",
  "description": "A demo of Remix running on Cloudflare workers.",
  "license": "MIT",
  "prisma": {
    "schema": "./db/schema.prisma",
    "seed": "node -r tsm -r tsconfig-paths/register scripts/seed.ts"
  },
  "scripts": {
    "cleanup": "rimraf .cache ./build ./public/build worker.js",
    "build:tailwind": "tailwindcss --minify -o app/styles/tailwind.css",
    "build:remix": "remix build",
    "build:worker": "cross-env NODE_ENV=production node -r tsm -r tsconfig-paths/register ./build.ts",
    "build": "yarn build:tailwind && yarn build:remix && yarn build:worker",
    "_dev:cypress": "cypress open",
    "dev:tailwind": "tailwindcss -o app/styles/tailwind.css --watch",
    "dev:miniflare": "miniflare --kv REDIRECTS --kv-persist --build-command \"node -r tsm -r tsconfig-paths/register ./build.ts\" --disable-cache --watch",
    "dev:remix": "remix watch",
    "dev": "cross-env NODE_ENV=development concurrently \"npm:dev:*\"",
    "type-check": "tsc",
    "lint": "eslint .",
    "lint:fix": "eslint --fix",
    "populatekv": "node ./scripts/populate-kv.js",
    "postinstall": "husky install && remix setup cloudflare-workers && yarn populatekv",
    "prebuild": "yarn cleanup",
    "test:cypress": "cypress run",
    "test": "start-server-and-test miniflare http-get://localhost:8787 test:cypress",
    "deploy": "wrangler publish",
    "start": "miniflare --kv REDIRECTS --kv-persist",
    "commit": "cz"
  },
  "dependencies": {
    "@cloudflare/kv-asset-handler": "^0.2.0",
    "@prisma/client": "3.5.0",
    "@remix-run/cloudflare-workers": "^0.21.0",
    "@remix-run/react": "^0.21.0",
    "classnames": "^2.3.1",
    "cloudinary-build-url": "0.2.1",
    "framer-motion": "5.3.1",
    "react": "next",
    "react-dom": "next",
    "react-router-dom": "^6.0.2",
    "remix": "^0.21.0",
    "remix-auth": "2.4.0"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^3.2.0",
    "@cloudflare/wrangler": "^1.19.5",
    "@remix-run/dev": "^0.21.0",
    "@tailwindcss/aspect-ratio": "0.3.0",
    "@tailwindcss/forms": "0.3.4",
    "@tailwindcss/typography": "0.4.1",
    "@testing-library/cypress": "^8.0.2",
    "@types/node": "16.11.9",
    "@types/react": "^17.0.35",
    "@types/react-dom": "^17.0.11",
    "@typescript-eslint/eslint-plugin": "5.4.0",
    "@typescript-eslint/parser": "5.4.0",
    "commitizen": "4.2.4",
    "concurrently": "^6.4.0",
    "cross-env": "^7.0.3",
    "cypress": "^9.0.0",
    "cz-emoji": "1.3.1",
    "daisyui": "^1.16.2",
    "esbuild": "^0.13.14",
    "eslint": "8.2.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-import-resolver-typescript": "2.5.0",
    "eslint-plugin-cypress": "2.12.1",
    "eslint-plugin-import": "2.25.3",
    "eslint-plugin-jest": "25.2.4",
    "eslint-plugin-jest-dom": "3.9.2",
    "eslint-plugin-jest-formatting": "3.1.0",
    "eslint-plugin-prettier": "4.0.0",
    "eslint-plugin-react": "7.27.1",
    "eslint-plugin-react-hooks": "4.3.0",
    "eslint-plugin-simple-import-sort": "7.0.0",
    "execa": "6.0.0",
    "husky": "7.0.4",
    "jest": "27.3.1",
    "lint-staged": "12.0.3",
    "miniflare": "^1.4.1",
    "patch-package": "^6.4.7",
    "prettier": "2.4.1",
    "prettier-plugin-prisma": "3.5.0",
    "pretty-quick": "3.1.2",
    "prisma": "^3.5.0",
    "rimraf": "^3.0.2",
    "start-server-and-test": "^1.14.0",
    "tailwindcss": "2.2.19",
    "tasuku": "1.0.2",
    "ts-jest": "27.0.7",
    "tsconfig-paths": "3.11.0",
    "tsm": "2.1.4",
    "typescript": "4.5.2"
  },
  "lint-staged": {
    "*.{js,ts,tsx}": [
      "yarn lint --fix"
    ]
  },
  "config": {
    "commitizen": {
      "path": "cz-emoji"
    }
  },
  "engines": {
    "node": ">=14"
  },
  "sideEffects": false,
  "packageManager": "yarn@3.1.0"
}
